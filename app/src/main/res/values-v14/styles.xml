<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ActionBarSB" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid</item>
    </style>

    <style name="ButtonSB" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/sb_btn_default_holo_dark</item>
    </style>

    <style name="ImageButtonSB" parent="android:Widget.Holo.ImageButton">
        <item name="android:background">@drawable/sb_btn_default_holo_dark</item>
    </style>

    <style name="SeekBarSB" parent="android:Widget.Holo.SeekBar">
        <item name="android:progressDrawable">@drawable/sb_scrubber_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/sb_scrubber_progress_horizontal_holo_dark</item>
        <item name="android:thumb">@drawable/sb_scrubber_control_selector_holo_dark</item>
    </style>

    <style name="ToggleSB" parent="android:Widget.Holo.Button.Toggle">
        <item name="android:background">@drawable/sb_btn_toggle_holo_dark</item>
    </style>

</resources>